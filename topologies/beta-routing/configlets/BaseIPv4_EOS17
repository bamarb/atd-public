hostname TRFL-CLIENT
!
interface Ethernet1
   description Uplink to TRFL for Client Simulation
   no switchport
   ip address 10.1.17.17/24
!
interface Loopback0
   ip address 17.17.17.17/32
   ipv6 address 2001:17::17/128
!
interface Management1
   ip address 192.168.0.26/24
   no lldp transmit
   no lldp receive
!
ip route 1.1.1.1/32 10.1.17.1
ip route 6.6.6.6/32 10.1.17.1
ip route 10.6.14.0/24 10.1.17.1