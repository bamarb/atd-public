interface Ethernet4
   description WRRN
   no switchport
   ip address 10.3.5.3/24
   ipv6 address 2001:3:5::3/64
   ip ospf cost 100
   ip ospf network point-to-point
   ip ospf area 0.0.0.0
   ipv6 ospf cost 100
   ipv6 ospf network point-to-point
   ipv6 ospf 1 area 0.0.0.0
   ip pim sparse-mode
!
interface Ethernet5
   description Unused
   no switchport
   shutdown
!
interface Ethernet6
   description Unused
   no switchport
   shutdown
!
interface Loopback0
   ip address 3.3.3.3/32
   ipv6 address 2001:3::3/128
   ipv6 ospf 1 area 0.0.0.0
!
interface Management1
   ip address 192.168.0.12/24
   no lldp transmit
   no lldp receive
!
! OSPF Config
!
router ospf 1
   router-id 3.3.3.3
   passive-interface default
   no passive-interface Ethernet1
   no passive-interface Ethernet3
   no passive-interface Ethernet4
   network 3.3.3.3/32 area 0.0.0.0
   max-lsa 12000
!
ipv6 router ospf 1
   router-id 3.3.3.3
   passive-interface default
   no passive-interface Ethernet1
   no passive-interface Ethernet3
   no passive-interface Ethernet4
!
! Locally Originated Routes
!
ip route 3.3.3.0/24 Null0
!
ipv6 route 3:3::/64 Null0
!
! Initial ISIS Configuration
!
router isis 1
   net 49.0001.0000.0003.00
   is-type level-2
   log-adjacency-changes
   !
   address-family ipv4 unicast
   !
   address-family ipv6 unicast
!
interface Loopback0
   isis enable 1
   isis passive
!
interface Ethernet1
   isis enable 1
   isis circuit-type level-2
   isis metric 100
   isis network point-to-point
!
! SR Config
!
mpls ip
!
router isis 1
   !
   segment-routing mpls
      router-id 3.3.3.3
      no shutdown
!
interface Loopback0
   node-segment ipv4 index 103
!
! BGP Configuration
!
router bgp 33362
   neighbor ibgp-full-ipv4 peer group
   neighbor ibgp-full-ipv4 remote-as 33362
   neighbor ibgp-full-ipv4 update-source Loopback0
   neighbor ibgp-full-ipv4 send-community
   neighbor ibgp-full-ipv4 maximum-routes 0
   neighbor ibgp-full-ipv6 peer group
