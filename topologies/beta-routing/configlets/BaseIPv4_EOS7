hostname MHT-ISP
!
interface Ethernet1
   description KRLS
   no switchport
   ip address 10.3.7.7/24
   ipv6 address 2001:3:7::7/64
!
interface Ethernet2
   description Unused
   no switchport
   shutdown
!
interface Ethernet3
   description Unused
   no switchport
   shutdown
!
interface Ethernet4
   description Unused
   no switchport
   shutdown
!
interface Loopback0
   ip address 7.7.7.7/32
   ipv6 address 2001:7::7/128
!
interface Management1
   ip address 192.168.0.16/24
   no lldp transmit
   no lldp receive