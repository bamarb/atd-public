hostname EGFK
!
interface Ethernet1
   description WRRN
   no switchport
   ip address 10.5.6.6/24
   ipv6 address 2001:5:6::6/64
!
interface Ethernet2
   description Unused
   no switchport
   shutdown
!
interface Ethernet3
   description MPLS
   no switchport
   ip address 10.6.13.6/24
   ipv6 address 2001:6:13::6/64
!
interface Ethernet4
   description TRFL
   no switchport
   ip address 10.1.6.6/24
   ipv6 address 2001:1:6::6/64
!
interface Ethernet5
   description Unused
   no switchport
   shutdown
!
interface Ethernet6
   description Unused
   no switchport
   shutdown
!
interface Loopback0
   ip address 6.6.6.6/32
   ipv6 address 2001:6::6/128
!
interface Management1
   ip address 192.168.0.15/24
   no lldp transmit
   no lldp receive